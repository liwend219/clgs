    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
    border-bottom: 0.001rem solid #CCCCCC;
}


.icon_search {
    background: url("../images/icon_search.png") no-repeat center;
    background-size: cover;
