body {
    background: #FFFFFF;
}

#container {
    margin-bottom: 1rem;
}

/*头部搜索框*/
/*获取焦点前*/
#back_nav {
    background-color: #242428;
    width: 100%;
    height: 0.88rem;
    overflow: hidden;
    line-height: 0.88rem;
    padding: 0 0 0 0.15rem;
    display: flex;
    align-items: center;
}
#back_nav .back_btn {
    float: left;
    width: 0.2rem;
    height: 0.36rem;
    margin-right: 0.2rem;
}


#search {
    width: 83%;
    padding: 0.14rem 0 0.14rem 0.54rem;
    background-color: #ffffff;
    outline: none;
    border: none;
    border-radius: 0.5rem;
    color: #575658;
    font-size: 0.28rem;
    letter-spacing: 0.01rem;
    float: left;
}

.icon_search {
    background: url("../images/icon_search.png") no-repeat center;
    background-size: cover;
    width: 0.4rem;
    height: 0.4rem;
    position: relative;
    left: -0.5rem;
}

.icon_del {
    background: url("../images/deeel.png") no-repeat center;
    background-size: cover;
    width: 0.45rem;
    height: 0.45rem;
    position: relative;
    left: -0.5rem;
}

#font {
    display: inline-block;
    width: 17%;
    height: 0.88rem;
    float: right;
    font-size: 0.32rem;
    color: #FFFFFF;
    text-align: center;
}

.HotSearch {
    width: 100%;
    padding: 0 0.3rem;
}

.HotSearch p {
    padding: 0.2rem 0;
    font-size: 0.33rem;
    line-height: 0.33rem;
    border-bottom: 0.01rem solid #e7e7e6;
    color: #393939;
}

.one {
    margin-top: 0.1rem;
}

.one li {
    width: 33.3%;
    float: left;
    font-size: 0.22rem;
    color: #9f9f9c;
    height: 0.5rem;
    line-height: 0.5rem;
}

.one li span {
    display: inline-block;
    width: 0.33rem;
    height: 0.33rem;
    background: #cccccc;
    font-size: 0.22rem;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    margin-right: 0.1rem;
    color: #FFFFFF;
}

li div {
    display: inline-block;
    font-size: 0.22rem;
    color: #9f9f9c;
    height: 0.5rem;
    line-height: 0.5rem;
}

.his {
    padding: 0 0.3rem;
}

#history {
    width: 100%;
    float: left;
    padding: 0.2rem 0;
    font-size: 0.33rem;
    line-height: 0.33rem;
    border-bottom: 0.01rem solid #e7e7e6;
    color: #393939;
}

#icon_del {
    float: right;
    width: 0.32rem;
    height: 0.32rem;
}

.his_font {
    margin-right: 0.3rem;
    font-size: 0.22rem;
    color: #9f9f9c;
}

.box_his {
    width: 100%;
    position: absolute;
    top: 4.3rem;
}

/*得到焦点后显示de*/
.box_focus {
    width: 100%;
    /*display: none;*/
    z-index: 9;
    padding: 0 0.3rem;
}

.box_focus .font_big {
    padding: 0.2rem 0;
    font-size: 0.33rem;
    line-height: 0.33rem;
    border-bottom: 0.01rem solid #e7e7e6;
    color: #161313;
}

.search_msg button {
    display: inline-block;
    width: 0.9rem;
    height: 0.33rem;
    border: 0.02rem solid #d63f2b;
    color: #d63f2b;
    font-size: 0.20rem;
    background: #FFFFFF;
    font-family: "微软雅黑";
    border-radius: 0.3rem;
    float: right;
    text-align: center;
}

.search_msg {
    width: 100%;
    font-size: 0.25rem;
    color: #393939;
    padding: 0.2rem 0rem;
    border-bottom: 0.01rem solid #e7e7e6;
}

.symbol, .code {
    font-size: 0.25rem;
    color: #393939;
}

.sss li:last-child {
    border-bottom: none;
}

.font_msg li {
    border-bottom: 0.01rem solid #e7e7e6;
    padding: 0.2rem 0;
}

.font_msg:last-child {
    border-bottom: none;
}

#information {
    font-size: 0.27rem;
    color: #393939;
}

#name_data {

}

#name_data span {

    font-size: 0.15rem;
    color: #9f9f9c;
}

#name_p {
    margin-top: 0.2rem;
    text-align: right;
}

