*{
	box-sizing: border-box;
}
body{
	background: #e6e6e7;
}
#back_nav {
    background-color: #FFFFFF;
    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
    border-bottom: 0.01rem solid #e6e6e7;
     display: flex;
     justify-content: center;
}

#back_nav .back_btn {
    width: 0.2rem;
    height: 0.36rem;
    margin: 0.26rem 0 0 0.28rem;
    margin-left: 0;
    position: relative;
    left: -0.2rem;
}

#back_nav .box{
   width: 3rem;
   height:0.6rem ;
   border: 0.01rem solid #CCCCCC;
   margin-top: 0.13rem;
   border-radius: 0.1rem;
   display: inline-block;
}
#back_nav .box li{
	float: left;
	width:50%;
	line-height: 0.58rem;
	font-size: 0.3rem;
}
#back_nav .box li:first-child{
	border-radius: 0.08rem 0 0 0.08rem;
}
#back_nav .box .icp{
	border-radius:0 0.08rem 0.08rem 0;
}
.active{
	background: #E34440;
	color: #FFFFFF;
}
.clear{
	clear: both;
}
#right{
    font-size: 0.3rem;
    color: #393939;
    margin-right: 0.2rem;
}
#back_nav .detailed {
    float: right;
    font-size: 0.3rem;
    color: #393939;
    margin: 0 0.3rem;
}
.shares{
	width: 100%;
	height: 1.4rem;
	background: #FFFFFF;
	/*margin-top: 0.3rem;*/
	padding: 0.2rem 0.3rem;
	display: flex;
	justify-content: center;
}
.shares .left{
	/* width: 50%; */
	text-align: left;
	margin-left:0;
}
.shares .left .sp1{
	font-size: 0.3rem;
	color: #393939;
}

.shares .h3{
	font-size: 0.45rem;
	color: #393939;
}
.shares .right{
	/* width: 50%; */
	margin-right:0;
	text-align: right;
}
.shares .h2{
	font-size: 0.55rem;
	color: rgb(21, 165, 122);
	font-weight: 600;
}
.shares .right .sp2{
	color: rgb(21, 165, 122);
}
#todaydata{
	width: 100%;
	background: #FFFFFF;
	border-top: 0.01rem solid #e7e7e6;
}
#todaydata ul{
	width: 100%;
	display: flex;
	justify-content: center;
	padding: 0.1rem 0;
}
#todaydata li{
	width: 25%;
	text-align: center;
}
#todaydata li p{
	font-size: 0.32rem;
}

.k_box{
	width: 100%;
	height: 0.5rem;
	margin-top: 0.2rem;
}
.k_box .first li{
	font-size: 0.32rem;
	width: 20%;
	color: #393939;
	float: left;
	text-align: center;
	height: 0.6rem;
	margin-top: 0.1rem;
	margin-bottom:0.15rem;
	border-bottom: 0.01rem solid #FFFFFF;
}
.k_graph{
	width: 100%;
	/* height: 5.5rem; */
	background: #FFFFFF;
	padding-top: 0.7rem;
}
#Diskdata{
	width: 100%;
	height: 4.12rem;
	
	padding:0.8rem 0.3rem;
}
#Diskdata ul{
	width: 33.3%;
	float: left;
}
#Diskdata ul li{
	text-align: left;
	height: 0.8rem;
	color: #393939;
	font-size: 0.3rem;
	padding: 0 0.1rem;
}
.button{
      	width: 100%;
      	height: 1rem;
      	background: #e0433b;
      	color: #FFFFFF;
      	text-align: center;
      	line-height: 1rem;
      	font-size: 0.36rem;
      	position: fixed;
      	bottom: 0;
	}
	  
/*提示信息*/
#tip {
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    font-size: 0.24rem;
    padding: 0.1rem 0.2rem;
    background: rgba(0, 0, 0, 0.5);
    color: #ffffff;
    border-radius: 0.1rem;
    text-align: center;
    z-index: 99;
}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none !important;
}

input[type="number"] {
	-moz-appearance: textfield;
	outline:none;
	border:none;
}
#inquiry_modelX {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.3);
    width: 100%;
    height: 100%;
}

#inquiry_modelX .model_content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: #ffffff;
    border-radius: 0.3rem;
    width: 80%;
}
#inquiry_modelX .model_content h3 {
    font-size: 0.34rem;
    color: #393939;
    text-align: center;
    padding: 0.24rem 0;
    border-bottom:  0.01rem solid #e0e0e0;
}
#inquiry_modelX .model_content .model_option {
    border-top: 0.01rem solid #e0e0e0;
    overflow: hidden;
}

#inquiry_modelX .model_content .model_option span {
    float: left;
    width: 50%;
    text-align: center;
    padding: 0.3rem 0;
    font-size: 0.32rem;
    letter-spacing: 0.12rem;
    font-weight: 600;
    cursor: pointer;
}

#inquiry_modelX .model_content .model_option .sure {
    border-left: 0.01rem solid #e0e0e0;
    color: #0e80fe;
}
#inquiry_modelX .model_content .bbooxx{
	padding: 0 0.3rem;
}
#inquiry_modelX .model_content .bbooxx p{
	line-height: 1.5rem;
	color: #393939;
	text-align: center;
}