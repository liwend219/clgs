body {
    background: #e6e6e7;
}

#container {
    text-align: center;
}

/* 头部导航 */
#container #back_nav {
    background: url("../images/nav_bar.png") no-repeat center;
    background-size: cover;
    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
}

#container #back_nav .back_btn {
    float: left;
    width: 0.2rem;
    height: 0.36rem;
    margin: 0.26rem 0 0 0.28rem;
}

#container #back_nav .title {
    margin-left: -0.48rem;
    font-size: 0.34rem;
    color: #ffffff;
    letter-spacing: 0.03rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 0.6rem;
}

/*预期到账收益*/
#profit_box {
    background: url("../images/details_bg.png") no-repeat center;
    background-size: cover;
    padding: 0.27rem 0 0.95rem 0;
    text-align: center;
}

#profit_box span {
    display: block;
    color: #ffffff;
}

#profit_box .profit_text {
    font-size: 0.24rem;
}

#profit_box .profit_money {
    font-size: 0.62rem;
}

#profit_box .profit_money .profit_company {
    font-style: normal;
}

/*产品详情*/
#product_details .product_box {
    background: #ffffff;
    overflow: hidden;
    padding: 0.14rem 0.3rem;
    border-bottom: 0.01rem solid #ededed;
}

#product_details .product_box div {
    float: left;
    width: 33.3%;
    text-align: center;
}

#product_details .product_box div p {
    font-size: 0.2rem;
    color: #9c9c9c;
    margin-bottom: 0.11rem;
}

#product_details .product_box div span {
    font-size: 0.32rem;
    color: #393939;
}

/*倒计时*/
#count_down {
    background: #ffffff;
    padding: 0.2rem 0;
    font-size: 0.2rem;
    color: #393939;
}

#count_down span {
    color: #ff5a5a;
    font-size: 0.2rem;
}

#count_down .quick_spell {
    color: #393939;
    font-size: 0.2rem;
    margin-left: 0.1rem;
}

/*产品内容*/
#release_box {
    background: #e6e6e7;
}

#release_box ul {
    background: #ffffff;
    margin-top: 0.33rem;
}

#release_box ul li {
    padding: 0.15rem 0;
    overflow: hidden;
    border-bottom: 0.01rem solid #ededed;
    text-align: left;
}

#release_box ul li h3 {
    float: left;
    font-size: 0.26rem;
    color: #393939;
    width: 50%;
    padding-left: 0.3rem;
    font-weight: normal;
}

#release_box ul li span {
    float: left;
    width: 50%;
    font-size: 0.26rem;
    color: #393939;
    position: relative;
}

#release_box .reward_box .reward_discount {
    color: #2984ef;
}

/*拼单策略*/
#strategy {
    background: #ffffff;
    margin-top: 0.33rem;
}

#strategy .strategy_title {
    padding: 0.15rem 0 0.15rem 0.3rem;
    font-size: 0.26rem;
    color: #393939;
    text-align: left;
    font-weight: normal;
}

#strategy .strategy_text {
    padding: 0.15rem 0.3rem;
    font-size: 0.26rem;
    width: 100%;
    color: #393939;
    text-align: left;
    word-wrap: break-word;
}

/*参与拼单按钮*/
#product_details .fight_btn {
    margin: 0.33rem 0;
    width: 80%;
    background-color: #d63f2b;
    text-align: center;
    height: 0.88rem;
    border-radius: 0.5rem;
    border: none;
    outline: none;
    font-size: 0.32rem;
    color: #ffffff;
    cursor: pointer;
}

/*平仓按钮*/
#product_details .close_btn {
    margin: 0.33rem 0;
    width: 80%;
    background-color: #d63f2b;
    text-align: center;
    height: 0.88rem;
    border-radius: 0.5rem;
    border: none;
    outline: none;
    font-size: 0.32rem;
    color: #ffffff;
    cursor: pointer;
}

/*参与拼单模态框*/
#joinFight_model {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.1);
    width: 100%;
    height: 100%;
}

#joinFight_model .model_content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: #ffffff;
    border-radius: 0.3rem;
    width: 85%;
}

#joinFight_model .model_content h3 {
    font-size: 0.34rem;
    color: #393939;
    text-align: center;
    padding: 0.24rem 0;
}

#joinFight_model .model_content ul li {
    overflow: hidden;
    border-top: 0.01rem solid #e0e0e0;
    padding: 0.16rem 0.6rem;
}

#joinFight_model .model_content ul li h2 {
    float: left;
    font-size: 0.26rem;
    font-weight: normal;
    width: 50%;
}

#joinFight_model .model_content ul li span {
    float: left;
    font-size: 0.26rem;
    width: 50%;
}

#joinFight_model .model_content ul li input {
    float: left;
    font-size: 0.26rem;
    width: 50%;
    outline: none;
    border: none;
}

#joinFight_model .model_content ul .cost_box .cost {
    color: #9c9c9c;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

#joinFight_model .model_content .model_option {
    border-top: 0.01rem solid #e0e0e0;
    overflow: hidden;
}

#joinFight_model .model_content .model_option span {
    float: left;
    width: 50%;
    text-align: center;
    padding: 0.3rem 0;
    font-size: 0.32rem;
    letter-spacing: 0.12rem;
    font-weight: 600;
    cursor: pointer;
}

#joinFight_model .model_content .model_option .sure {
    border-left: 0.01rem solid #e0e0e0;
    color: #0e80fe;
}

/*平仓模态框*/
#closePosition_model {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.1);
    width: 100%;
    height: 100%;
}

#closePosition_model .model_content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: #ffffff;
    border-radius: 0.3rem;
    width: 80%;
}

#closePosition_model .model_content h3 {
    font-size: 0.34rem;
    color: #393939;
    text-align: center;
    padding: 0.24rem 0;
}

#closePosition_model .model_content ul li {
    overflow: hidden;
    border-top: 0.01rem solid #e0e0e0;
    padding: 0.16rem 0.6rem;
}

#closePosition_model .model_content ul li h2 {
    float: left;
    font-size: 0.26rem;
    font-weight: normal;
    width: 50%;
}

#closePosition_model .model_content ul li span {
    float: left;
    font-size: 0.26rem;
    width: 50%;
}

#closePosition_model .model_content ul li input {
    font-size: 0.26rem;
    border: none;
    outline: none;
    margin-top: 0.04rem;
    float: left;
    width: 50%;
}

#closePosition_model .model_content .model_option {
    border-top: 0.01rem solid #e0e0e0;
    overflow: hidden;
}

#closePosition_model .model_content .model_option span {
    float: left;
    width: 50%;
    text-align: center;
    padding: 0.3rem 0;
    font-size: 0.32rem;
    letter-spacing: 0.05rem;
    cursor: pointer;
    color: #9c9c9c;
}

#closePosition_model .model_content .model_option .sure {
    border-left: 0.01rem solid #e0e0e0;
    color: #0e80fe;
    letter-spacing: 0.12rem;

}

/*提示信息*/
#tip {
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    font-size: 0.24rem;
    padding: 0.1rem 0.2rem;
    background: rgba(0, 0, 0, 0.5);
    color: #ffffff;
    border-radius: 0.1rem;
    text-align: center;
    z-index: 99;
}