<body>
    <div>
        <div id="back_nav">
            <a href="javascript:history.back(-1);">
                <img src="images/back.png" alt="" class="back_btn">
            </a>
            <span class="title" style="margin-left: 0;color: #393939">策略详情</span>
        </div>
        <div class="cell-box">
            <div class="cell-item">
                策略交易号
            </div>
            <div class="cell-item" id="sid" style="background: #fff">
                CL104084　T+D(ZH73)
                <a href="xieyi_jy.html" style="float:right;color:#1583FE;font-size:0.28rem">合约协议</a>
            </div>
            <!-- <div class="cell-item">
                交易合作
            </div>
            <div class="item">
                <div class="item-border item-1-pad">投资人　　 &nbsp;&nbsp;<span>crystal(UID:692053)</span></div>
                <div class="item-1-pad">点买人　　 &nbsp;&nbsp;<span>sky1991(UID:692053)</span></div>
            </div> -->
           <div class="cell-item">
                交易明细
            </div>
            <div class="item">
                <div class="item-flex">
                    <div class="item-border item-1-pad" style="width:50%">股票名称　 <span id="stockName">祥源文化</span></div>
                    <div class="item-border item-1-pad" style="width:50%">交易本金　 <span id="jiaoyibenjin">604.00</span></div>
                </div>
                <div class="item-flex">
                    <div class="item-border item-1-pad" style="width:50%">买入价格　 <span id="buyPriceDeal">6.040</span></div>
                    <div class="item-border item-1-pad" style="width:50%">买入类型　 <span>市价买入</span></div>
                </div>
                <div class="item-flex">
                    <div class="item-border item-1-pad" style="width:50%">卖出价格　 <span id="sellPriceDeal">6.040</span></div>
                    <div class="item-border item-1-pad" style="width:50%">买出类型　 <span>即时成交</span></div>
                </div>
                <div class="item-flex">
                    <div class="item-border item-1-pad" style="width:50%">买入时间　 <span id="dealTime">2018-04-24</span></div>
                    <div class="item-border item-1-pad" style="width:50%">卖出时间　 <span>2018-04-24</span></div>
                </div>
                <div class="item-flex">
                    <div class="item-border item-1-pad" style="width:50%">持仓天数　 <span>1天</span></div>
                    <div class="item-border item-1-pad" style="width:50%">交易数量　 <span>100股</span></div>
                </div>
                <div class="item-flex">
                    <div class="item-1-pad" style="width:50%">综合费　　 <span>28.00</span></div>
                    <div class="item-1-pad" style="width:50%">递延费　　 <span>0.00</span></div>
                </div>
            </div>
            <div class="cell-item">
                盈利分配
            </div>
            <div class="item">
                <div class="item-border item-1-pad">策略盈亏　　&nbsp;<span style="color:rgb(21, 165, 122)">-4.00</span></div>
                <div class="item-border item-1-pad">亏损赔付　　&nbsp;<span style="color:rgb(21, 165, 122)">-4.00</span></div>
                <div class="item-1-pad">盈利分配　　&nbsp;<span>0.00</span></div>
            </div>
            <div class="cell-item">
                资金结算
            </div>
            <div class="item">
                <div class="item-border item-1-pad">冻结　　　　 &nbsp;&nbsp;<span>604.00</span></div>
                <div class="item-border item-1-pad">扣减　　　　 &nbsp;&nbsp;<span>-4.00</span></div>
                <div class="item-1-pad">退回　　　　 &nbsp;&nbsp;<span>600.00</span></div>
            </div>
        </div>
        <br><br>
    </div>
    <script>
        (function(){
            var tDetail=JSON.parse(sessionStorage.getItem('tDetail'))
            console.log(tDetail)
            $('#sid').text('CL'+tDetail.sid)
            $('#stockName').text(tDetail.stockName)
            $('#jiaoyibenjin').text((tDetail.buyPriceDeal*tDetail.volumeDeal).toFixed(2))
            $('#buyPriceDeal').text(tDetail.buyPriceDeal.toFixed(2))
            $('#sellPriceDeal').text(tDetail.sellPriceDeal.toFixed(2))
            var t = parseInt(tDetail.dealTime.slice(6,-2))
            var dealTime = setTime(t)
            console.log(t)
           
            console.log(dealTime)
            $('#dealTime').text(dealTime)
        })()
        function setTime(t){
            var date = new Date(t)
            Y = date.getFullYear() + '-';
            M = (date.getMonth()+1 < 10 ? '0'+(date.getMonth()+1) : date.getMonth()+1) + '-';
            D = (date.getDate() <10 ? '0'+(date.getDate()) : date.getDate());
            return Y+M+D
        }
    </script>
</body>
