* {
    margin: 0 auto;
    padding: 0;
    font-size: 14px;
    box-sizing: border-box;
    font-family: "微软雅黑" !important;
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}
body {
    background: #e6e6e7;
}

#back_nav {
    background-color: #FFFFFF;
    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
    /* border-bottom: 0.01rem solid #e7e7e6; */
    margin-bottom:0.2rem;
}

#back_nav .back_btn {
    float: left;
    width: 0.2rem;
    height: 0.36rem;
    margin: 0.26rem 0 0 0.28rem;
}

#back_nav .title {
    margin-left: 0.88rem;
    font-size: 0.34rem;
    color: #393939;
    letter-spacing: 0.03rem;
}

#back_nav .detailed {
    float: right;
    font-size: 0.3rem;
    color: #393939;
    margin: 0 0.3rem;
}