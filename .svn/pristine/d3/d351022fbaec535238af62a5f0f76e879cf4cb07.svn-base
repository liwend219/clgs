* {
    margin: 0 auto;
    padding: 0;
    font-size: 14px;
    box-sizing: border-box;
    font-family: "微软雅黑" !important;
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}
body {
    background: #e6e6e7;
}

#back_nav {
    background-color: #FFFFFF;
    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
    /* border-bottom: 0.01rem solid #e7e7e6; */
    margin-bottom:0.2rem;
}

#back_nav .back_btn {
    float: left;
    width: 0.2rem;
    height: 0.36rem;
    margin: 0.26rem 0 0 0.28rem;
}

#back_nav .title {
    margin-left: 0.88rem;
    font-size: 0.34rem;
    color: #393939;
    letter-spacing: 0.03rem;
}

#back_nav .detailed {
    float: right;
    font-size: 0.3rem;
    color: #393939;
    margin: 0 0.3rem;
}

.box{
	width: 100%;
	padding: 0 0.3rem;
	background-color: #FFFFFF;
}
.box ul {
	width: 100%;
	background-color: #FFFFFF;
	margin-top: 0.3rem;
}
.box ul li{
  padding: 0.18rem 0;
    overflow: hidden;
    border-bottom: 0.01rem solid #ededed;
    cursor: pointer;

}
.box ul li:last-child{
	border: none;
}
.box ul li span{
	display: inline-block;
	width: 50%;
	text-align: left;
	font-size: 0.3rem;
	color: #393939;
}
.detail{
    padding:0.3rem 0.3rem 0 0.3rem;
    background: #fff;
}
.detail-1{
   
    line-height:40px;
}
.detail-1 span input{
    height:35px;
    padding:0 15px;
    background-color: #F5F5F4;
    border:none;
}
.detail-2{
    display: -webkit-flex; 
    display: flex;
    justify-content:center;
    align-items:center;
    flex-wrap: wrap;
    margin:0.18rem 0;
}
.detail-2-item{
    width:23%;
    height:0.6rem;
    text-align: center;
    line-height: 0.6rem;
    background: #fff;
    border:1px solid #E7E7E6;
    margin-bottom:0.18rem;
    border-radius: 0.09rem;
}
.detail-3 div{
    /* line-height: 38px; */
   width: 100%;
   height:0.8rem;
   line-height: 0.8rem;
   padding: 0.15rem 0;
   	color: #393939;
    /*border-bottom:1px solid #E7E7E6;*/
}
.detail-3-item  .font{
	display: inline-block;
	width:20%;
	height: 0.5rem;
	line-height: 0.5rem;
	background: #FFFFFF;
	font-size: 0.3rem;
	float: left;
	color: #393939;
}
.t1-select{
    text-align: center;
    display: none;
    /* padding:10px 0 3px; */
    /* border-top:1px solid #E7E7E6; */
}
.button{
    width: 100%;
    height: 1rem;
    background: #e0433b;
    color: #FFFFFF;
    text-align: center;
    line-height: 1rem;
    font-size: 0.36rem;
    position: fixed;
    bottom: 0;
}
.zhanwei{
    width: 100%;
    height: 1rem;
}
.detail-4{
    padding:0.2rem;
    background: #fff;
}
.detail-4-title{
    font-size: 0.32rem;
    padding-bottom:0.15rem;
}
.detail-4 .detail-4-item{
    color:rgb(89, 89, 207);
    font-size:0.26rem;
}
.tic{
	display: block;
	width:1rem;
	height: 0.5rem;
	line-height: 0.5rem;
	background: #FFFFFF;
	float: right;
	margin-left: 0.2rem;
	text-align: center;
	color: #393939;
	border: 0.01rem solid #E7E7E6;
	font-size: 0.24rem;
	border-radius: 0.09rem;
}
.active{
	border: 0.01rem solid #e0433b;
}
 input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
    }
    input[type="number"]{
        -moz-appearance: textfield;
    }