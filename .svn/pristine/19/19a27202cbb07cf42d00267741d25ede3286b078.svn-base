				if(parseFloat($(this).val())<parseFloat(sessionStorage.getItem('tShuliang'))||parseFloat($(this).val())%100 != 0){
					$('#dianmaijine').text(0)
					$('#zhiying').text(0)
					$('#zhisun').text(0)
					$('#lybzj').text('履约保证金　　0')
