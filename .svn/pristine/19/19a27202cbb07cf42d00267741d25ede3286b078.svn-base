			$('.div-left').append('<div>卖3</div><div class="maiJiage" style="padding-right:20px">'+Math.round(data[0].卖三报价*100)/100+'</div><div style="padding-right:20px">'+Math.round(data[0].卖三*100)/100+'</div>')
