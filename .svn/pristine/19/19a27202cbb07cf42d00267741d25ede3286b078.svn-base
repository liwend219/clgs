			    zongliang2 = (Math.round(zongliang1*100)/100).toFixed(2)+"亿";
				console.log(zongliang2)
			}
			else if(zongliang > 10000){
				zongliang1 = zongliang/10000;
				zongliang2 = (Math.round(zongliang1*100)/100).toFixed(2)+"万";
				console.log(zongliang2)
			}else if(zongliang < 10000){
