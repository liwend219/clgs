						$('#k_img').css('display','block')
						$('#pankous').css('display','none')
					}else if($(this)[0].innerText == '日k'){
						$('#k_img').attr('src','http://image.sinajs.cn/newchart/daily/n/'+sessionStorage.getItem('$_code').toLowerCase()+'.gif')
