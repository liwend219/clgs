* {
    margin: 0 auto;
    padding: 0;
    font-size: 14px;
    box-sizing: border-box;
    font-family: "微软雅黑" !important;
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}
body {
    background: #e6e6e7;
}

#back_nav {
    background-color: #FFFFFF;
    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
    border-bottom: 1px solid rgb(223, 223, 223);
}

#back_nav .back_btn {
    float: left;
    width: 0.2rem;
    height: 0.36rem;
    margin: 0.26rem 0 0 0.28rem;
}

#back_nav .title {
    margin-left: 0.88rem;
    font-size: 0.34rem;
    color: #393939;
    letter-spacing: 0.03rem;
}

#back_nav .detailed {
    float: right;
    font-size: 0.3rem;
    color: #393939;
    margin: 0 0.3rem;
}
.clear{
	clear: both;
}
.tab-bar{
    width:100%;
    height:0.9rem;
    display: -webkit-flex; 
    display: flex;
    justify-content:space-around;
    align-items:center;
    background: #fff;
}
.bar-left,.bar-right{
    width:1.8rem;
    height:0.9rem;
    text-align: center;
    line-height: 0.9rem;
    border:none;
    background-color: #fff;
    outline:none;
}
.bar-btn-click{
    border-bottom:2px solid #393939;
}
.nav-prompt{
    display: none;
    padding:10px 15px 5px 15px;
    background-color: #fff;
}
.prompt-header{
    position: relative;
    width:1.8rem;
    height: 23px;
    background: #DE4239;
    margin-left:0px;
    padding-left:10px;
    color:#fff;
    line-height: 23px;
}
.prompt-text{
    padding:5px 0;
    line-height: 22px;
    color:#666666;
}
.transform-box{
    position: absolute;
    right:0;
    width:23px;
    height:23px;
    top:0;
    right:-10px;
    background: #fff;
    transform:rotate(45deg);
    -ms-transform:rotate(45deg); 	/* IE 9 */
    -moz-transform:rotate(45deg); 	/* Firefox */
    -webkit-transform:rotate(45deg); /* Safari 和 Chrome */
    -o-transform:rotate(45deg); 	/* Opera */
}
.nav-detail{
    width:100%;
    background: #fff;
}
.detail-item{
    height:0.9rem;
    line-height: 0.9rem;
    padding:0 15px;
    border-bottom:1px solid rgb(223, 223, 223)
}
.detail-item-1{
    width:100vw;
    padding:0 15px;
}
.detail-item-1-1{
    width:100vw;
    padding:0 15px;
}
.detail-item-2{
    height:1rem;
    line-height: 1rem;
    display: -webkit-flex; 
    display: flex;
    justify-content:center;
    align-items:center;
}
.detail-item-1 div{
    border-bottom:1px solid rgb(223, 223, 223);
    display: -webkit-flex; 
    display: flex;
    justify-content:space-around;
    align-items:center;
    flex-wrap:wrap;
    padding-top:10px;
}
.detail-item-1 div .item-1{
    width:33%;
    color:#666666;
    padding-bottom:10px;
}
.detail-item-1 div .item-2{
    width: 49%;
    color: #666666;
    padding-bottom:10px;
}
.item-cell{
    /* border-bottom:1px solid rgb(223, 223, 223); */
    display: -webkit-flex; 
    display: flex;
    justify-content:space-around;
    align-items:center;
    flex-wrap:wrap;
    padding:5px;
    border-bottom:1px solid rgb(223, 223, 223);
}
.item-3{
    width: 49%;
    color: #666666;
    padding:5px;
    /* border-bottom:1px solid #ccc; */
}
.item-btn{
    width:45%;
    border:none;
    border-radius:5px;
    padding:6px 0px;
    color:#fff;
    outline:none;
}

/* 模态框 */
#inquiry_model {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.1);
    width: 100%;
    height: 100%;
}

#inquiry_model .model_content {
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: #ffffff;
    border-radius: 0.3rem;
    width: 80%;
}

#inquiry_model .model_content h3 {
    font-size: 0.34rem;
    color: #393939;
    text-align: center;
    padding: 0.24rem 0;
}

#inquiry_model .model_content ul li {
    overflow: hidden;
    padding: 0.16rem 0.6rem;
}

#inquiry_model .model_content ul li h2 {
    float: left;
    font-size: 0.26rem;
    font-weight: normal;
    width: 50%;
}

#inquiry_model .model_content ul li span {
    float: left;
    font-size: 0.26rem;
    width: 50%;
}

#inquiry_model .model_content ul .cost_box .cost {
    color: #d63f2b;
}

#inquiry_model .model_content .model_option {
    border-top: 0.01rem solid #e0e0e0;
    overflow: hidden;
}

#inquiry_model .model_content .model_option span {
    float: left;
    width: 50%;
    text-align: center;
    padding: 0.3rem 0;
    font-size: 0.32rem;
    letter-spacing: 0.12rem;
    font-weight: 600;
    cursor: pointer;
}
#inquiry_model .model_content .model_option .sure {
    border-left: 0.01rem solid #e0e0e0;
    color: #0e80fe;
}
.ipu{
    width:85%;
    height:40px;
    border-bottom:1px solid #ccc;
    display: -webkit-flex; 
    display: flex;
    justify-content:space-between;
    align-items:center;
}
.ipu input{
    width:50%;
    border:none;
    outline:none;
}