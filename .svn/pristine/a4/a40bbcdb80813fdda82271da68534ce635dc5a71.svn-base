$(function () {
    //跳转标识
    var identification = sessionStorage.getItem("跳转标识");

    if (identification == "register_agreement") {
        $("#back_nav .title").text("用户注册协议");
        $("#register_agreement").css("display", "block");
    }

    if (identification == "adviser_agreement") {
        $("#back_nav .title").text("投资顾问协议");
        $("#adviser_agreement").css("display", "block");
    }
});