* {
    margin: 0 auto;
    padding: 0;
    font-size: 14px;
    box-sizing: border-box;
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}

body {
    background: #FFFFFF;
}

#back_nav {
    background-color: #F7F7F8;
    width: 100%;
    height: 0.88rem;
    text-align: center;
    overflow: hidden;
    line-height: 0.88rem;
}

.fixd {
    position: fixed;
    top: 0;
}

.top {
    width: 100%;
    height: 0.88rem;
}

#back_nav .back_btn {
    float: left;
    width: 0.2rem;
    height: 0.36rem;
    margin: 0.26rem 0 0 0.28rem;
}

#back_nav .title {
    margin-left: -0.48rem;
    font-size: 0.34rem;
    color: #393939;
    letter-spacing: 0.03rem;
}

.lists ul {
    background: #ffffff;

}

.lists ul li {
    padding: 0.2rem 0;
    margin: 0 0.3rem;
    overflow: hidden;
    border-bottom: 0.01rem solid #ededed;
    cursor: pointer;
}

.lists ul li:last-child {
    border-bottom: none;
}

.lists ul li span, a {
    font-size: 0.32rem;
    color: #393939;
    vertical-align: middle;
}

.lists ul li i {
    background: url("../images/arrow.png") no-repeat center;
    background-size: cover;
    width: 0.16rem;
    height: 0.27rem;
    float: right;
    margin-top: 0.13rem;
}

.lists p {
    padding: 0.2rem 0;
    margin: 0 0.3rem;
    overflow: hidden;
    font-size: 0.32rem;
    color: #393939;
    vertical-align: middle;
}

.lists .security {
    margin-top: 0.3rem;
}

/*问题页面--三个*/
.font {
    width: 100%;
    padding: 0 0.3rem;
    background: #FFFFFF;
    margin-top: 0.3rem;
}

.font p:first-child {
    padding: 0.2rem 0;
    font-size: 0.32rem;
    color: #393939;
}

.font p:last-child {
    padding: 0.5rem 0;
    font-size: 0.25rem;
    color: #393939;
}

.boxfeed p {
    padding: 0.2rem 0;
    margin: 0 0.3rem;
    overflow: hidden;
    font-size: 0.32rem;
    color: #393939;
    vertical-align: middle;
}

#text {
    resize: none;
    width: 100%;
    height: 3rem;
    border: none;
    outline: none;
    font-size: 0.32rem;
    line-height: 0.4rem;
}

#sub_btn {
    width: 100%;
    height: 0.9rem;
    background-color: #D63F2B;
    margin-top: 0.3rem;
    border: none;
    outline: none;
    color: #FFFFFF;
    font-size: 0.32rem;
}

#phone {
    width: 100%;
    font-size: 0.3rem;
    border: none;
    outline: none;
    padding: 0.2rem 0.3rem;
}

/*帮助中心协议规则*/

.gz_box {
    padding: 0.3rem 0.3rem;

}

.gz_box h3 {
    font-size: 0.4rem;
    text-align: center;
    margin-top: 0.3rem;
}

.h_sm {
    font-size: 0.35rem;
    margin: 0.2rem 0;
    font-weight: 600;
}

.h_sm_5 {
    font-size: 0.32rem;
    margin: 0.2rem 0;
    font-weight: 500;
}

.p_con {
    font-size: 0.24rem;
    line-height: 0.30rem;
}

.margin {
    margin-bottom: 0.1rem;
}

.red {
    color: red;
}

.xhx {
    text-decoration: underline
}


/*首页css*/
.con img{
	width: 100%;
}
.font{
	padding:0 0.2rem;
	width: 100%;
	height: 1.3rem;
	border-bottom: 0.01rem solid #ededed;
	position: relative;
	top: -0.4rem;
}
.font img{
	width: 0.88rem;
	height: 0.88rem;
	position: absolute;
	top: 0.2rem;
	left: 0.2rem;
}
#right{
	width: 85%;
	float: right;
}
/*#right p{
	margin: 0;padding: 0;
}*/
#right .p_c{
	font-size: 0.4rem;
	margin: 0;padding: 0;
	margin-top: 0.15rem;
	width: 75%;
	float: left;
	color: #393939;
}
#right .p_c_1{
	font-size: 0.26rem;
	margin: 0;padding: 0;
	margin-top: 0.06rem;
	width: 75%;
	float: left;
	color: #C7C7CC;
}
#icon_z{
	display: inline-block;
	width: 1.2rem;
	height: 0.54rem;
	line-height: 0.54rem;
	text-align: center;
	border-radius: 0.5rem;
	background: #ff5a5a;
	float: right;
	color: #FFFFFF;
	position: relative;
	top: -0.25rem;
	font-size: 0.3rem;
}
.font_jianjie{
	padding:0 0.2rem;
	width: 100%;
	height: 3rem;
	position: relative;
	top: -0.4rem;
	border-bottom: 0.01rem solid #ededed;
}
.font_jianjie .p_c{
	font-size: 0.4rem;
	margin: 0;padding: 0;
	margin-top: 0.15rem;
	width: 100%;
	float: left;
	color: #393939;
	margin-bottom: 0.1rem;
}
.font_jianjie .p_c span{
	float: right;
	position: relative;
	top: 0.15rem;
	color: #C7C7CC;
}
.font_jianjie .content{
	line-height: 0.38rem;
	color: #333333;
}
