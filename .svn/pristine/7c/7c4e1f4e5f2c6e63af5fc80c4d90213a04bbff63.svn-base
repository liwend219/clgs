.info_box ul li .rank_id {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
}

/*提示信息*/
#tip {
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    font-size: 0.24rem;
    padding: 0.1rem 0.2rem;
    background: rgba(0, 0, 0, 0.5);
    color: #ffffff;
    border-radius: 0.1rem;
    text-align: center;
    z-index: 99;
}

.state_tip {
    width: 100%;
    text-align: center;
}

.state_tip p {
    text-align: center;
    font-size: 0.32rem;
    color: #393939;
}

.state_tip img {
    width: 3.5rem;
    margin-top: 4rem;
}

.button_goma{
	margin-top: 0.2rem;
			background: #D63F2B;
        	padding: 0.1rem 0.3rem;
        	outline: none;
        	border: none;
        	border-radius: 0.3rem;
        	color: #FFFFFF;
}
